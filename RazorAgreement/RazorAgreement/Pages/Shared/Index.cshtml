@model IEnumerable<Models.Agreement>
@{ Layout = "_Layout"; ViewBag.Title = "All Agreements";}

<h2>All Agreements</h2>

<table class="table table-sm table-striped table-bordered m-2">
    <thead>
        <tr>
            <th>AgreementId</th>
            <th>Name</th>
            <th>Description</th>
            <th>Amount</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var r in Model)
        {
            <tr>
                <td>@r.AgreementId</td>
                <td>@r.Name</td>
                <td>@r.Description</td>
                <td>@r.Amount</td>
            </tr>
        }
    </tbody>
</table>
